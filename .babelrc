{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "components": "./src/components",
          "constants": "./src/constants",
          "store": "./src/store",
          "util": "./src/util"
        }
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": ["react-refresh/babel"]
    },
    "production": {
      "plugins": ["lodash"]
    }
  }
}
