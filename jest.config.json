{
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/index.js",
    "!src/**/*.connected.js"
  ],
  "coverageReporters": ["lcov", "text-summary"],
  "moduleNameMapper": {
    "\\.(css|less|scss|sass)$": "identity-obj-proxy"
  },
  "reporters": ["jest-dot-reporter"],
  "setupFiles": ["./test-setup.js"],
  "testMatch": ["**/?(*.)+(test).js"],
  "testPathIgnorePatterns": ["<rootDir>/node_modules/"],
  "transform": {
    "^.+\\.js$": "babel-jest"
  }
}
