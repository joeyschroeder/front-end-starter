{
  "name": "front-end-starter",
  "version": "0.0.1",
  "description": "A front-end starter application including React, Redux, Webpack and more!",
  "main": "./src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --env production",
    "clean": "rm -rf docs/ && echo \"\\x1b[102m\\x1b[1m CLEAN \\x1b[0m\" docs directory removed",
    "eslint": "./node_modules/.bin/eslint src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "pre-publish": "npm run --silent stylelint && npm run --silent eslint && npm run --silent clean && npm run --silent build",
    "start": "./node_modules/.bin/webpack-dev-server --inline --hot --env development",
    "stylelint": "./node_modules/.bin/stylelint 'src/**/*.scss' --config .stylelintrc && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" No StyleLint SCSS errors to fix",
    "stylelint:fix": "./node_modules/.bin/stylelint --fix 'src/**/*.scss' --config .stylelintrc && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" StyleLint SCSS errors fixed",
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:changed": "./node_modules/.bin/jest -o --config ./jest.config.json",
    "test:coverage": "./node_modules/.bin/jest --config ./jest.config.json --coverage"
  },
  "author": {
    "name": "Joey Schroeder",
    "email": "schrjos@gmail.com"
  },
  "license": "MIT",
  "pre-commit": {
    "run": [
      "eslint",
      "stylelint",
      "test:changed"
    ],
    "silent": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joeyschroeder/front-end-starter.git"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/register": "^7.13.8",
    "autoprefixer": "^9.8.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "babel-preset-joey": "0.0.4",
    "copy-webpack-plugin": "^6.0.1",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.22.0",
    "eslint-config-joey": "^0.1.8",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-dot-reporter": "^1.0.14",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.13.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^20.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "classnames": "^2.2.6",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0"
  }
}
