{
  "name": "front-end-starter",
  "version": "0.0.1",
  "description": "A front-end starter application including React, Redux, Webpack and more!",
  "main": "./src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --env production",
    "clean": "rm -rf docs/ && echo \"\\x1b[102m\\x1b[1m CLEAN \\x1b[0m\" docs directory removed",
    "eslint": "./node_modules/.bin/eslint src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "pre-publish": "npm run --silent stylelint && npm run --silent eslint && npm run --silent clean && npm run --silent build",
    "start": "./node_modules/.bin/webpack serve --inline --hot --env development",
    "stylelint": "./node_modules/.bin/stylelint 'src/**/*.scss' --config .stylelintrc && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" No StyleLint SCSS errors to fix",
    "stylelint:fix": "./node_modules/.bin/stylelint --fix 'src/**/*.scss' --config .stylelintrc && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" StyleLint SCSS errors fixed",
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:changed": "./node_modules/.bin/jest -o --config ./jest.config.json",
    "test:coverage": "./node_modules/.bin/jest --config ./jest.config.json --coverage"
  },
  "author": {
    "name": "Joey Schroeder",
    "email": "schrjos@gmail.com"
  },
  "license": "MIT",
  "pre-commit": {
    "run": [
      "eslint",
      "stylelint",
      "test:changed"
    ],
    "silent": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joeyschroeder/front-end-starter.git"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/register": "^7.16.0",
    "autoprefixer": "^10.4.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.1.0",
    "copy-webpack-plugin": "^8.0.0",
    "css-loader": "^5.1.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.22.0",
    "eslint-config-airbnb": "^19.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-dot-reporter": "^1.0.14",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "postcss": "^8.3.11",
    "postcss-loader": "^5.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.12.0",
    "stylelint-config-standard": "^21.0.0",
    "terser-webpack-plugin": "^5.2.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "animate.css": "^4.1.1",
    "classnames": "^2.3.1",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "lodash": "^4.17.21",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.0"
  }
}
