{
  "name": "front-end-starter",
  "version": "0.0.1",
  "description": "A front-end starter application including React, Redux, Webpack and more!",
  "main": "./src/index.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --env production",
    "clean": "rm -rf dist/ && echo \"\\x1b[102m\\x1b[1m CLEAN \\x1b[0m\" Dist directory removed",
    "eslint": "./node_modules/.bin/eslint src/ --max-warnings=0 && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" No JavaScript ESLint errors to fix",
    "eslint:fix": "./node_modules/.bin/eslint --fix src/ && echo \"\\x1b[102m\\x1b[1m ESLINT \\x1b[0m\" ESLint JavaScript errors fixed",
    "start": "./node_modules/.bin/webpack-dev-server --inline --hot --env development",
    "stylelint": "./node_modules/.bin/stylelint 'src/**/*.scss' --config .stylelintrc && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" No StyleLint SCSS errors to fix",
    "stylelint:fix": "./node_modules/.bin/stylelint --fix 'src/**/*.scss' --config .stylelintrc && echo \"\\x1b[102m\\x1b[1m STYLELINT \\x1b[0m\" StyleLint SCSS errors fixed",
    "test": "./node_modules/.bin/jest --config ./jest.config.json",
    "test:changed": "./node_modules/.bin/jest -o --config ./jest.config.json",
    "test:coverage": "./node_modules/.bin/jest --config ./jest.config.json --coverage"
  },
  "author": {
    "name": "Joey Schroeder",
    "email": "schrjos@gmail.com"
  },
  "license": "MIT",
  "pre-commit": {
    "run": [
      "eslint",
      "stylelint",
      "test:changed"
    ],
    "silent": true
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.artisanpartners.com/projects/GIMP/repos/component-library/browse"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-constant-elements": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.6.2",
    "autoprefixer": "^9.6.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-react-pure-class-to-function": "^1.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.5.1",
    "eslint-config-joey": "^0.1.3",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-dot-reporter": "^1.0.10",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "path": "^0.12.7",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.10.2",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "animate.css": "^3.7.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.5.2",
    "font-awesome": "^4.7.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-hot-loader": "^4.12.14",
    "react-redux": "^7.1.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.3.0"
  }
}
